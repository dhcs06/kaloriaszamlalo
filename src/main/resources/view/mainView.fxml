<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<VBox fx:controller="controller.MainController"
      xmlns:fx="http://javafx.com/fxml"
      alignment="center"
      maxWidth="100"
      fx:id="vbox"
      minWidth="500"
      minHeight="500"
      spacing="10.0">
    <children>
        <Button mnemonicParsing="false" text="My Week" onAction="#onMyWeekClick"/>
        <Button mnemonicParsing="false" text="Food list" onAction="#onFoodListClick"/>
        <Button mnemonicParsing="false" text="Exit" onAction="#onExitClick"/>
    </children>
    <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </VBox.margin>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
